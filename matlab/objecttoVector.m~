function [stateVector] = objecttoVector(state)
    stateVector = [];
    count = 1;
    for target=state.target_robots
        stateVector = [stateVector; target.state];
        count
        count = count+1;
    end 
    count = 0
    for obstacle=state.obstacle_robots
        stateVector = [stateVector; obstacle.state];
        count
    end
end
